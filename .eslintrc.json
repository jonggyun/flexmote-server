{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": 1
  },
  "overrides": [
    {
      "files": "src/entity/**/*.ts",
      "rules": {
        "no-unused-vars": 0
      }
    }
  ]
}
